<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping SYSTEM "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.emisi.model">

	<class name="Paciente" table="paciente_ob">
		
		<!-- Los datos de los pacientes estÃ¡n protegidos -->
		<id name="id" column="id_paciente" />
		
		<property name="sexo" column="sexo" />
		
		<bag name="estudios" cascade="all-delete-orphan" lazy="false">
        	<key column="id_paciente" />
        	<one-to-many class="com.emisi.model.Estudio" />
        </bag>
	
	</class>
	
</hibernate-mapping>